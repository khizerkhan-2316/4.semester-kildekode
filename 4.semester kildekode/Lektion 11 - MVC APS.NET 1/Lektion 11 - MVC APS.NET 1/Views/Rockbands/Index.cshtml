
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        <h1>Great Rockbands</h1>
        <ul>
            @{ 
                for(int i = 0; i < ViewBag.rockBands.Length; i++)
                {
                    <li>@(i + 1). @ViewBag.rockBands[i]</li>
                }
            }
        </ul>
    </div>
</body>
</html>
